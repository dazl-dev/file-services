{
  "name": "@file-services/memory",
  "description": "An in-memory, sync/async, file system implementation.",
  "version": "0.3.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "ts-build ./src --cjs --esm",
